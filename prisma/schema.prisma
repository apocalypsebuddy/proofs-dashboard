generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Proof {
  id             String   @id @default(uuid())
  date           DateTime @default(now())
  customerId     String
  printerId      String
  batchId        String
  description    String
  customerName   String
  printerName    String
  printBackUrl   String?
  printFrontUrl  String?
  resourceId     String
}
